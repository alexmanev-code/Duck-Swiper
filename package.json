{
  "name": "random-duck-gallery-app",
  "version": "1.0.0",
  "description": "A simple web application to display random duck images using the random-d.uk API, built with Express.js, EJS, Tailwind CSS, and AJAX.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:css": "tailwindcss -i ./public/css/style.css -o ./public/css/output.css --watch"
  },
  "keywords": [
    "express",
    "ejs",
    "tailwind-css",
    "ajax",
    "api",
    "javascript",
    "mobile-first"
  ],
  "author": "Aleksandar Manev",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1"
  }
}
